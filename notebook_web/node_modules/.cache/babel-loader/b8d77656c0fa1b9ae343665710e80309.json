ire('@/assets/sidebar-img/sidebar-1.jpg'),\n    require('@/assets/sidebar-img/sidebar-2.jpg'),\n    require('@/assets/sidebar-img/sidebar-3.jpg'),\n    require('@/assets/sidebar-img/sidebar-4.jpg'),\n  ],\n  notifications: [],\n  rtl: false,\n}\n\nconst mutations = make.mutations(state)\n\nconst actions = {\n  fetch: ({ commit }) => {\n    const local = localStorage.getItem('vuetify@user') || '{}'\n    const user = JSON.parse(local)\n\n    for (const key in user) {\n      commit(key, user[key])\n    }\n\n    if (user.dark === undefined) {\n      commit('dark', window.matchMedia('(prefers-color-scheme: dark)'))\n    }\n  },\n  update: ({ state }) => {\n    if (!IN_BROWSER) return\n\n    localStorage.setItem('vuetify@user', JSON.stringify(state))\n  },\n}\n\nconst getters = {\n  dark: (state, getters) => {\n    return (\n      state.dark ||\n      getters.gradient.indexOf('255, 255, 255') === -1\n    )\n  },\n  gradient: state => {\n    return state.gradients[state.drawer.gradient]\n  },\n  image: state => {\n    return state.drawer.image === '' ? state.drawer.image : state.images[state.drawer.image]\n  },\n}\n\nexport default {\n  namespaced: true,\n  state,\n  mutations,\n  actions,\n  getters,\n}\n"]}]}