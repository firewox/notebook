CA4CU,MA5CV,E,CA8CA;;AACA,SAASR,UAAT,CAAqBS,YAArB,EAAmC;AACjC,MAAIC,YAAY,GAAG,EAAnB;AACA,MAAIC,IAAI,GAAG,IAAIC,IAAJ,EAAX;AACAD,EAAAA,IAAI,CAACE,OAAL,CAAaJ,YAAb;AACAC,EAAAA,YAAY,IAAIC,IAAI,CAACG,WAAL,EAAhB,CAJiC,CAIE;;AACnCJ,EAAAA,YAAY,IAAI,MAAMK,QAAQ,CAACJ,IAAD,CAA9B,CALiC,CAKI;;AACrCD,EAAAA,YAAY,IAAI,MAAMM,MAAM,CAACL,IAAD,CAA5B,CANiC,CAME;;AACnC,SAAOD,YAAP;AACD,C,CACD;;;AACA,SAASK,QAAT,CAAmBJ,IAAnB,EAAyB;AACvB,MAAIM,KAAK,GAAG,EAAZ;AACAA,EAAAA,KAAK,GAAGN,IAAI,CAACI,QAAL,KAAkB,CAA1B,CAFuB,CAEK;;AAC5B,MAAIE,KAAK,GAAG,EAAZ,EAAgB;AACdA,IAAAA,KAAK,GAAG,MAAMA,KAAd;AACD;;AACD,SAAOA,KAAP;AACD,C,CACD;;;AACA,SAASD,MAAT,CAAiBL,IAAjB,EAAuB;AACrB,MAAIO,GAAG,GAAG,EAAV;AACAA,EAAAA,GAAG,GAAGP,IAAI,CAACQ,OAAL,EAAN;;AACA,MAAID,GAAG,GAAG,EAAV,EAAc;AACZA,IAAAA,GAAG,GAAG,MAAMA,GAAZ;AACD;;AACD,SAAOA,GAAP;AACD","sourcesContent":["import Vue from 'vue'\nimport App from './App.vue'\nimport router from './router'\nimport vuetify from './plugins/vuetify'\nimport './plugins'\nimport store from './store'\nimport { sync } from 'vuex-router-sync'\nimport CKEditor from 'ckeditor4-vue'\nimport ElementUI from 'element-ui'\nimport 'element-ui/lib/theme-chalk/index.css'\nimport './http'\n// import VueResource from 'vue-resource'\n\n// Vue.use(VueResource)\n\nVue.use(ElementUI)\n\nVue.use(CKEditor)\n\n// Vue.prototype.$bus = new Vue()\n\nVue.config.productionTip = false\n\nsync(store, router)\n\nnew Vue({\n  data: () => ({\n    user: {\n      userId: -1,\n      username: '',\n      email: '',\n      gender: '',\n      birthday: '',\n      created: '',\n      categoryId: [],\n      notebookId: [],\n      trashId: [],\n    },\n  }),\n  router,\n  vuetify,\n  store,\n  render: h => h(App),\n  created () {\n    var _this = this\n    if (this.$route.path === '/') {\n      this.$http.post('/login/indexHtml.do',\n        {})\n        .then(function (response) {\n          if (!response.data.flag) {\n            confirm(response.data.errorMsg)\n            location.href = 'components/login'\n          } else {\n            _this.user.username = response.data.data.username\n            _this.user.userId = response.data.data.userId\n            _this.user.email = response.data.data.email\n            _this.user.gender = response.data.data.gender\n            _this.user.birthday = dateFormat(response.data.data.birthday)\n            _this.user.created = dateFormat(response.data.data.created)\n          }\n        })\n        .catch(function (error) {\n          console.log(error)\n        })\n    }\n  },\n  beforeCreate () {\n    Vue.prototype.$bus = this\n  },\n}).$mount('#app')\n\n// 日期格式转换函数\nfunction dateFormat (longTypeDate) {\n  var dateTypeDate = ''\n  var date = new Date()\n  date.setTime(longTypeDate)\n  dateTypeDate += date.getFullYear() // 年\n  dateTypeDate += '-' + getMonth(date) // 月\n  dateTypeDate += '-' + getDay(date) // 日\n  return dateTypeDate\n}\n// 返回 01-12 的月份值\nfunction getMonth (date) {\n  var month = ''\n  month = date.getMonth() + 1 // getMonth()得到的月份是0-11\n  if (month < 10) {\n    month = '0' + month\n  }\n  return month\n}\n// 返回01-30的日期\nfunction getDay (date) {\n  var day = ''\n  day = date.getDate()\n  if (day < 10) {\n    day = '0' + day\n  }\n  return day\n}\n"]}]}