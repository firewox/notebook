       <v-row>\n                  <v-col>\n                    <v-text-field\n                      v-model=\"user.verificationCode\"\n                      type=\"text\"\n                      :error-messages=\"verificationRules(user.verificationCode)\"\n                      label=\"验证码\"\n                      required\n                    ></v-text-field>\n                  </v-col>\n                  <v-col>\n                    <v-btn\n                      class=\"ma-2\"\n                      outlined\n                      color=\"indigo\"\n                      @click=\"fun_verification\"\n                    >\n                      发送邮箱验证码\n                    </v-btn>\n                  </v-col>\n                </v-row>\n                <div style=\"height: 20px\"></div>\n                <el-button\n                  type=\"primary\"\n                  :disabled=\"!valid\"\n                  @click=\"validate\"\n                >注册</el-button>\n\n                <el-button\n                  type=\"info\"\n                  @click=\"reset\"\n                >重置</el-button>\n                <el-button\n                  type=\"info\"\n                  @click=\"fun_login\"\n                >登录</el-button>\n\n              </v-form>\n            </el-card>\n          </el-main>\n        </el-container>\n      </el-col>\n    </el-row>\n    <el-row\n      type=\"flex\"\n      justify=\"center\"\n    >\n      <!-- justify 对齐方式 -->\n      <el-col\n        :span=\"6\"\n        style=\"height:100px\"\n      >\n      </el-col>\n    </el-row>\n    <el-row\n      type=\"flex\"\n      justify=\"center\"\n    >\n      <!-- justify 对齐方式 -->\n      <el-col\n        :span=\"6\"\n        style=\"height:100px\"\n      >\n      </el-col>\n    </el-row>\n    <el-row\n      type=\"flex\"\n      justify=\"center\"\n    >\n      <!-- justify 对齐方式 -->\n      <el-col\n        :span=\"6\"\n        style=\"height:100px\"\n      >\n      </el-col>\n    </el-row>\n    <el-row\n      type=\"flex\"\n      justify=\"center\"\n    >\n      <!-- justify 对齐方式 -->\n      <el-col\n        :span=\"6\"\n        style=\"height:100px\"\n      >\n      </el-col>\n    </el-row>\n  </div>\n</template>\n\n<script>\n  export default {\n    name: 'Register',\n    data: () => ({\n      user: {\n        username: '',\n        gender: '',\n        birthday: '2022-02-01',\n        email: '',\n        password: '',\n        confirmedPassword: '',\n        verificationCode: '',\n      },\n      valid: true,\n      email: '',\n      emailRules: [\n        v => !!v || '邮箱不能为空！',\n        v => /.+@.+\\..+/.test(v) || '邮箱格式非法！',\n      ],\n      password: '',\n      passwordRules: [\n        v => !!v || '密码不能为空！',\n      ],\n    }),\n    methods: {\n      validate () {\n        this.$refs.form.validate()\n        if (this.$refs.form.validate()) {\n          this.fun_register()\n        }\n      },\n      reset () {\n        this.$refs.form.reset()\n      },\n      fun_register: function () {\n        var _this = this\n        this.$http.post('/register/registerUser.do', {\n          username: _this.user.username,\n          gender: _this.user.gender,\n          birthday: _this.user.birthday,\n          email: _this.user.email,\n          password: _this.user.password,\n          verificationCode: _this.user.verificationCode,\n        }).then(function (response) {\n          if (response.data.flag) {\n            _this.$message({\n              message: response.data.errorMsg,\n              type: 'success',\n            })\n            _this.$router.push({ path: '/components/login' }).catch(err => { err = null })\n          } else {\n            _this.$message({\n              message: response.data.errorMsg,\n              type: 'warning',\n            })\n          }\n        })\n      },\n      confirmedPasswordRules: function (v) {\n        if (v.length === 0 && this.user.password.length !== 0) {\n          return '密码不能为空！'\n        } else if (v !== this.user.password) {\n          return '两次的密码不一致！'\n        } else {\n          return ''\n        }\n      },\n      verificationRules: function (v) {\n        if (v.length === 0 && this.user.confirmedPassword.length !== 0) {\n          return '验证码不能为空！'\n        }\n      },\n      fun_login: function () {\n        this.$router.push({ path: '/components/login' }).catch(err => { err = null })\n      },\n      fun_verification: function () {\n        var _this = this\n        this.$http.post('/register/checkEmailExist.do', {\n          email: _this.user.email,\n        }).then(function (response) {\n          if (response.data.flag === true) {\n            _this.$message({\n              message: '邮箱验证码5分钟内有效',\n              type: 'success',\n            })\n          } else {\n            _this.$message({\n              message: '邮箱已被注册！',\n              type: 'warning',\n            })\n          }\n        }).catch(function (e) {\n          console.log(e)\n        })\n      },\n    },\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  #submit{\n    color: #FFFFFF;\n    background: rgb(24,103,192);\n  }\n  #reset{\n    color: #FFFFFF;\n    background: rgb(251,140,0);\n  }\n  #appR{\n    background: url(../assets/register.jpg) no-repeat;\n    background-size: cover;\n    height: 100%;\n  }\n  .el-card{\n    border-radius:30px;\n    width: 580px;\n    display:flex;\n    justify-content:center;\n  }\n  .el-row {\n    margin-bottom: 20px;\n  }\n  .v-form {\n    width: 380px;\n  }\n</style>\n"]}]}