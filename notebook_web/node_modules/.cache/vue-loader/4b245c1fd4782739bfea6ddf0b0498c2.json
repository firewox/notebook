  <el-card\n              class=\"el-card\"\n              shadow=\"always\"\n            >\n              <h1 style=\"text-align: center;\">欢迎登陆</h1>\n              <el-divider></el-divider>\n              <v-form\n                ref=\"form\"\n                v-model=\"valid\"\n                lazy-validation\n              >\n                <v-text-field\n                  v-model=\"email\"\n                  :rules=\"emailRules\"\n                  label=\"邮箱\"\n                  required\n                ></v-text-field>\n\n                <v-text-field\n                  v-model=\"password\"\n                  :rules=\"passwordRules\"\n                  type=\"password\"\n                  label=\"密码\"\n                  required\n                ></v-text-field>\n                <div style=\"height: 20px\"></div>\n                <el-button\n                  type=\"primary\"\n                  :disabled=\"!valid\"\n                  @click=\"validate\"\n                >登录</el-button>\n\n                <el-button\n                  type=\"info\"\n                  @click=\"reset\"\n                >重置</el-button>\n\n                <el-button\n                  type=\"info\"\n                  @click=\"fun1\"\n                >注册</el-button>\n              </v-form>\n            </el-card>\n          </el-main>\n        </el-container>\n      </el-col>\n    </el-row>\n    <el-row\n      type=\"flex\"\n      justify=\"center\"\n    >\n      <!-- justify 对齐方式 -->\n      <el-col\n        :span=\"6\"\n        style=\"height:100px\"\n      >\n      </el-col>\n    </el-row>\n    <el-row\n      type=\"flex\"\n      justify=\"center\"\n    >\n      <!-- justify 对齐方式 -->\n      <el-col\n        :span=\"6\"\n        style=\"height:100px\"\n      >\n      </el-col>\n    </el-row>\n    <el-row\n      type=\"flex\"\n      justify=\"center\"\n    >\n      <!-- justify 对齐方式 -->\n      <el-col\n        :span=\"6\"\n        style=\"height:100px\"\n      >\n      </el-col>\n    </el-row>\n    <el-row\n      type=\"flex\"\n      justify=\"center\"\n    >\n      <!-- justify 对齐方式 -->\n      <el-col\n        :span=\"6\"\n        style=\"height:100px\"\n      >\n      </el-col>\n    </el-row>\n  </div>\n</template>\n\n<script>\n  export default {\n    name: 'Login',\n    data: () => ({\n      valid: true,\n      email: '',\n      emailRules: [\n        v => !!v || '邮箱不能为空！',\n        v => /.+@.+\\..+/.test(v) || '邮箱格式非法！',\n      ],\n      password: '',\n      passwordRules: [\n        v => !!v || '密码不能为空！',\n      ],\n    }),\n\n    methods: {\n      validate () {\n        this.$refs.form.validate()\n        if (this.$refs.form.validate()) {\n          var _this = this\n          this.$http.post('/login/loginUser.do', {\n            email: _this.email,\n            password: _this.password,\n          }).then(function (response) {\n            if (response.data.flag) {\n              // _this.$bus.emit('user_email', _this.email)\n              // console.log('_this.$that.user===' + _this.$that.user)\n              _this.$that.user.email = _this.email\n              _this.$that.user.userId = response.data.data.userId\n              _this.$that.user.username = response.data.data.username\n              _this.$that.user.gender = response.data.data.gender\n              _this.$that.user.birthday = response.data.data.birthday\n              _this.$that.user.created = response.data.data.created\n              // sessionStorage.setItem('user', JSON.stringify(_this.$that.user))\n              sessionStorage.setItem('email', JSON.stringify(_this.$that.user.email))\n              // sessionStorage.setItem('user', 'helloworld')\n              // console.log('sessionStorage.getItem===' + JSON.parse(sessionStorage.getItem('email')))\n              // alert('_this.$that.user.email===' + _this.$that.user.email)\n              _this.$message({\n                message: response.data.errorMsg,\n                type: 'success',\n              })\n              _this.$router.push({ path: '/' }).catch(err => { err = null })\n            } else {\n              _this.$message({\n                message: response.data.errorMsg,\n                type: 'warning',\n              })\n            }\n          })\n        }\n      },\n      reset () {\n        this.$refs.form.reset()\n      },\n      fun1: function () {\n        this.$router.push({ path: '/components/register' }).catch(err => { err = null })\n      },\n    },\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  #submit{\n    color: #FFFFFF;\n    background: rgb(24,103,192);\n  }\n  #reset{\n    color: #FFFFFF;\n    background: rgb(251,140,0);\n  }\n  #appL{\n    background: url(../assets/bg.jpg) no-repeat;\n    background-size: cover;\n    height: 100%;\n  }\n  .el-card{\n    border-radius:30px;\n    width: 580px;\n    display:flex;\n    justify-content:center;\n  }\n  .el-row {\n    margin-bottom: 20px;\n  }\n  .v-form {\n    width: 380px;\n  }\n</style>\n"]}]}